    "fullpage",
    "fullpagejs",
    "one_page",
    "sliding",
    "snap",
    "scroll",
    "swipe",
    "mousewheel",
    "wheel",
    "mouse",
    "sections",
    "slides",
    "slideshow"
  ],
  "files": [
    "vendors",
    "dist/fullpage.css",
